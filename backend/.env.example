AI_PROVIDER=groq
AI_TIMEOUT_SECONDS=90

# Seguranca da API (opcional)
# Deixe vazio para uso publico sem login.
APP_API_KEY=
MAX_PDF_SIZE_MB=5
MAX_JOB_DESCRIPTION_CHARS=15000

# Rate limit por minuto (por IP)
RATE_LIMIT_OPTIMIZE_PER_MINUTE=12
RATE_LIMIT_EXPORT_PER_MINUTE=20

# Captcha (Cloudflare Turnstile)
CAPTCHA_ENABLED=false
TURNSTILE_SECRET_KEY=

# Groq (recomendado: baixo custo / rapido)
GROQ_API_KEY=your_groq_key_here
GROQ_MODEL=llama-3.1-8b-instant

# Gemini (Google AI Studio)
GEMINI_API_KEY=
GEMINI_MODEL=gemini-1.5-flash

# OpenRouter (modelos free e baixo custo)
OPENROUTER_API_KEY=
OPENROUTER_MODEL=meta-llama/llama-3.1-8b-instruct:free

# OpenAI (opcional)
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o-mini

# Chunking para modelos com contexto menor
RESUME_CHUNK_MAX_CHARS=1100
RESUME_CHUNK_MIN_CHARS=260
RESUME_CHUNK_MAX_SELECTED=6

FRONTEND_ORIGIN=http://localhost:5173
